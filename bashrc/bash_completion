
_vimsessions()
{
  sessdir="$HOME/.vimcnk/.sessions/"
  if [ -d ${sessdir} ]; then
    COMPREPLY=( $(compgen -W "$(ls ${sessdir})" ${COMP_WORDS[COMP_CWORD]} ) )
  fi
}
complete -F _vimsessions vims

_upper_dir()
{
  if [ -d ${sessdir} ]; then
    COMPREPLY=( $(compgen -W "$(find ../ -maxdepth 1 -type d)" ${COMP_WORDS[COMP_CWORD]} ) )
  fi
}
complete -F _upper_dir u

